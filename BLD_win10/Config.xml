    Cyyz=2
    
    [口令]
    Password=耸
    
    [历史数据保存天数]
    DaysOfData=360
    
    [吹灰状态]
    ChHStatus=1
    
    ;=================吹灰报警时间（单位小时)======================
    [吹灰报警时间]
    ChHAlarmtime=8
    
