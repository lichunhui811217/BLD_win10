    RemotePort=n          ;UDP端口设置,如：8001
    RemoteHost=n          ;UDP远程主机地址,如：255.255.255.255-->
    <UDP Enable="0" SupportServer="0"  RemotePort="6803" RemoteHost="10.164.19.255"></UDP>

