    原串口通讯设置改为通用串口数据池.-->
    <SerialPorts>
      <SerialPort  
        PortName = "COM1"
        BaudRate = "19200"
        Parity = "Even"
        DataBits = "8"
        StopBits = "1"
        Handshake = "0"
        ReadTimeout = "500"
        WriteTimeout = "500"
                   
